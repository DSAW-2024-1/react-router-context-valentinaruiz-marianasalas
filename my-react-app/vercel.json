{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "my-react.app/src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/my-react.app/src/static/$1"
    },
    {
       "src": "/(.*)\\.png",
       "dest": "/my-react.app/src/$1.png"
     },
    {
      "src": "/(.*)",
      "dest": "/my-react.app/src/index.html"
    }
  ]
}
