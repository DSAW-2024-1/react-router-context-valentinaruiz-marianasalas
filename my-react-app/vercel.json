{
  "version": 2,
  "builds": [
    {
      "src": "my-react.app/src/main.jsx",
      "use": "@vercel/node"
    },
    {
      "src": "my-react.app/src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/logout",
      "dest": "/client/index.html"
    },
    {
      "src": "/contact",
      "dest": "/client/index.html"
    },
    {
      "src": "/overview",
      "dest": "/client/index.html"
    },
    {
      "src": "/login",
      "dest": "/client/index.html"
    }
  ],
  
    "rewrites":  [
      {"source": "/(.*)", "destination": "/"}
    ]
  
}

